    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "ims-lti": "^3.0.2",
    "passport": "^0.6.0",
    "passport-anonymous": "^1.0.1",
    "passport-custom": "^1.1.1",
