    "sync-json": "^1.0.2",
    "ts-jest": "28.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.2",
    "wait-on": "^7.0.1",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "dependencies": {
    "@angular/animations": "~15.1.0",
