    "@nrwl/webpack": "15.5.3",
    "@nrwl/workspace": "15.5.3",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "28.1.1",
    "@types/node": "18.7.1",
    "@types/passport-anonymous": "^1.0.3",
    "@types/passport-jwt": "^3.0.8",
