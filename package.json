{
  "name": "platon",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cisstech/platon"
  },
  "bugs": {
    "url": "https://github.com/cisstech/platon/issues"
  },
  "homepage": "https://github.com/cisstech/platon",
  "author": {
    "name": "Mamadou Cisse",
    "email": "mciissee@gmail.com"
